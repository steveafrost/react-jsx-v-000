  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "expect": "^1.20.2",
    "jsdom": "^9.4.1",
    "mocha": "^2.5.3",
    "mocha-multi": "^0.9.0",
    "node-inspector": "^0.12.8"
