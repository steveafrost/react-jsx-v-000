{
  "name": "react-jsx",
  "version": "0.1.0",
  "description": "Introduction to using JSX with React",
  "engines": {
    "node": "6.x",
    "npm": "3.x"
  },
  "main": "index.js",
  "scripts": {
    "debug": "node-debug --hidden node_modules _mocha --watch",
